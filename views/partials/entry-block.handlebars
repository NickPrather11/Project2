<li>
    <div class="row">
        <h5 class="col s11">{{title}}</h5>
        <button class="btn-small col s1" id="delete-entry" data-id="{{this.id}}"><i
                class="material-icons">cancel</i></button>
    </div>
    <div class="row">
        <div class="col s10 entryDescription">
            {{description}}
        </div>
        <div class="col s2">
            {{!-- <button class="edit-entry btn-small" data-id="{{this.id}}">EDIT/UPDATE</button> --}}
            {{!-- <button class="btn-small" id="delete-entry" data-id="{{this.id}}"><i
                    class="material-icons">cancel</i></button> --}}
        </div>
    </div>
    {{!-- <div class="row"> --}}
    {{#each tags}}
    <div class="chip tag" data-id="{{id}}"> {{this.name}}
        <i class="close material-icons">close</i>
    </div>
    {{/each}}
    {{!-- </div> --}}
</li>